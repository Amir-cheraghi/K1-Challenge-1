                                                    <button type="button" class="btn btn-danger"
                                                        onclick="deleteTask(this)" data-url="<%-el._id%>">Delete
                                                        Task</button>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <!-- /delete -->
                            </div>
                        </div>
                        <div class="card-footer">
                        </div>
